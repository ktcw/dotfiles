set nocompatible
filetype plugin indent on
set term=xterm-256color
colo delek
syntax on
set number
set laststatus=2
set nohlsearch
set incsearch
set ignorecase
set ruler
set autoindent
set tabstop=2
set softtabstop=2
set shiftwidth=2
set shiftround
set smarttab
set nowrap
set backspace=indent,eol,start
highlight LineNr ctermfg=darkgrey
